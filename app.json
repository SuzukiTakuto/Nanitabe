{
  "expo": {
    "name": "何食",
    "slug": "Nanitabe",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugin": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "何食に位置情報の使用を許可しますか？",
          "locationAlwaysPermission": "何食に位置情報の使用を許可しますか？",
          "locationWhenInUsePermission": "何食に位置情報の使用を許可しますか？"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ttaktt.Nanitabe",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "現在地の近くのお店を探すために位置情報は利用されます。",
        "NSLocationAlwaysUsageDescription": "現在地の近くのお店を探すために位置情報は利用されます。",
        "NSLocationWhenInUseUsageDescription": "現在地の近くのお店を探すために位置情報は利用されます。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "782978d7-a564-48f7-9c7e-d5375db40514"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-3382119718476117~9035800307"
  }
}
